{
  "name": "smart-kitchen-backend",
  "version": "1.0.0",
  "description": "Smart Kitchen Manager Backend API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@as-integrations/fastify": "^2.1.1",
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^7.2.4",
    "@fastify/multipart": "^7.6.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.9",
    "date-fns": "^3.0.6",
    "dotenv": "^17.2.3",
    "fastify": "^4.25.2",
    "form-data": "^4.0.5",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.4.0",
    "nodemailer": "^6.9.8",
    "openai": "^4.24.1",
    "sharp": "^0.33.1",
    "tesseract.js": "^5.0.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "eslint": "^9.15.0",
    "prettier": "^3.3.0",
    "prisma": "^6.19.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "kitchen",
    "inventory",
    "management",
    "api",
    "graphql",
    "fastify"
  ],
  "author": "Smart Kitchen Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}